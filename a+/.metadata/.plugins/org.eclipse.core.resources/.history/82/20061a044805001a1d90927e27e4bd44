package DAO;

import java.util.List;

import service.dto.WorkerDTO;

public class WorkerDAO {
	public List<WorkerDTO> getWorkerList();		// 전체 현직자 정보를 획득
	public int insertWorker(WorkerDTO w);	// 현직자정보를 추가
	public int updateWorker(WorkerDTO w);	// 현직자정보를 수정
	public int deleteWorker(int w_num);		// 현직자정보를 삭제
	public WorkerDTO getWorkerByName(String name);		// 현직자정보를 이름으로 찾음
	public WorkerDTO getWorkerById(String id);		// 현직자정보를 id로 찾음

}
