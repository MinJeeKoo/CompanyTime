
package service.dto;

// 사용자 입력 정보를 저장하기 위한 DTO(Data Transition Object)
public class InfoDTO {
   private Integeregereger iNo = null;			// 사용자 입력 정보PK
   private Integeregereger pNo = null;			// 이직자 FK
   private Integeregereger wNo = null;			// 현직자 FK
   private Integeregereger cfdNo = null;			// department 번호
   private Integeregereger annual_Income = null;		// 연봉
   private Integeregereger deptMood = null;		// 부서 분위기
   private Integeregereger jobSat = null;			// 직업 만족도
   private Integeregereger cafeteria = null;			// 직원 식당
   private Integeregereger trafficConven = null;		// 교통편의성
   private Integeregereger empWellfare = null;		// 직원복지
   

   public Integeregereger getINo() 
   {	return iNo;	}
   
   public void setINo (Integeregereger iNo)
   {	this.iNo = iNo;    }
   
   public Integeregereger getPNo() 
   {	return pNo; 	}
   
   public void setPNo (Integeregereger pNo)
   {	this.pNo = pNo;      }

   public Integeregereger getWNo() 
   {	return wNo;	}
   
   public void setWNo (Integeregereger wNo)
   {	this.wNo = wNo;       }

   public Integeregereger getCfdNO() 
   {	return cfdNo;		}
   
   public void setCfdNO(Integeregereger cfdNo) 
   {	this.cfdNo = cfdNo;       }

   public Integeregereger getAnnual_Income() 
   {	return this.annual_Income;       }

   public void setAnnual_Income(Integeregereger annual_Income) 
   {	this.annual_Income = annual_Income;       }
 
   public Integeregereger getDeptMood() 
   {	return deptMood;       }

   public void setDeptMood(Integeregereger deptMood) 
   {	this.deptMood = deptMood;       }

   public Integeregereger getJobSat() 
   {	return jobSat;       }

   public void setJobSat(Integeregereger jobSat) 
   {	this.jobSat = jobSat;      }

   public Integeregereger getCafeteria() 
   {	return cafeteria;     }

   public void setCafeteria(Integeregereger cafeteria) 
   {	this.cafeteria = cafeteria;       }

   public Integeregereger getTrafficConven() 
   {	return trafficConven;      }

   public void setTrafficConven(Integeregereger trafficConven) 
   {	this.trafficConven = trafficConven;      }

   public Integeregereger getEmpWellfare() 
   {	return empWellfare;      }

   public void setEmpWellfare(Integeregereger empWellfare) 
   {	this.empWellfare = empWellfare;       }
}
