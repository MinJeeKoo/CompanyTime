
package service.dto;

// 사용자 입력 정보를 저장하기 위한 DTO(Data Transition Object)
public class InfoDTO {
   private Integereger iNo = null;			// 사용자 입력 정보PK
   private Integereger pNo = null;			// 이직자 FK
   private Integereger wNo = null;			// 현직자 FK
   private Integereger cfdNo = null;			// department 번호
   private Integereger annual_Income = null;		// 연봉
   private Integereger deptMood = null;		// 부서 분위기
   private Integereger jobSat = null;			// 직업 만족도
   private Integereger cafeteria = null;			// 직원 식당
   private Integereger trafficConven = null;		// 교통편의성
   private Integereger empWellfare = null;		// 직원복지
   

   public Integereger getINo() 
   {	return iNo;	}
   
   public void setINo (Integereger iNo)
   {	this.iNo = iNo;    }
   
   public Integereger getPNo() 
   {	return pNo; 	}
   
   public void setPNo (Integereger pNo)
   {	this.pNo = pNo;      }

   public Integereger getWNo() 
   {	return wNo;	}
   
   public void setWNo (Integereger wNo)
   {	this.wNo = wNo;       }

   public Integereger getCfdNO() 
   {	return cfdNo;		}
   
   public void setCfdNO(Integereger cfdNo) 
   {	this.cfdNo = cfdNo;       }

   public Integereger getAnnual_Income() 
   {	return this.annual_Income;       }

   public void setAnnual_Income(Integereger annual_Income) 
   {	this.annual_Income = annual_Income;       }
 
   public Integereger getDeptMood() 
   {	return deptMood;       }

   public void setDeptMood(Integereger deptMood) 
   {	this.deptMood = deptMood;       }

   public Integereger getJobSat() 
   {	return jobSat;       }

   public void setJobSat(Integereger jobSat) 
   {	this.jobSat = jobSat;      }

   public Integereger getCafeteria() 
   {	return cafeteria;     }

   public void setCafeteria(Integereger cafeteria) 
   {	this.cafeteria = cafeteria;       }

   public Integereger getTrafficConven() 
   {	return trafficConven;      }

   public void setTrafficConven(Integereger trafficConven) 
   {	this.trafficConven = trafficConven;      }

   public Integereger getEmpWellfare() 
   {	return empWellfare;      }

   public void setEmpWellfare(Integereger empWellfare) 
   {	this.empWellfare = empWellfare;       }
}
